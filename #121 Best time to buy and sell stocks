class Solution {
public:
    int maxProfit(vector<int>& prices) {
       int maxprofit=0;
       int minPrice=prices[0];
       for(int i = 0; i<prices.size(); i++){
            if(prices[i] < minPrice){
                minPrice= prices[i];
            }
            else{
                int profit = prices[i]- minPrice;
                maxprofit= max(profit, maxprofit);
            }
            
       }
       return maxprofit;
    }
};
